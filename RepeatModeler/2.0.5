#%Module1.0#####################################################################
##
## RepeatModeler modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tRepeatModeler - Adds RepeatModeler to your path"
}

module-whatis	"RepeatModeler-2.0.5"

set      hpcc_modules    $::env(HPCC_MODULES)

if { [module-info mode load] || [module-info mode switch2] } {
    puts stdout "module load miniconda3; conda config --set env_prompt '(\$(echo {default_env} | perl -p -e \"s#.+pkgs/##; s#/envs?##;\")) '; conda activate ${hpcc_modules}/RepeatModeler/2.0.5;"
} elseif { [module-info mode remove] && ![module-info mode switch3] } {
    puts stdout "conda deactivate;"
}

