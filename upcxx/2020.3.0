#%Module1.0#####################################################################
##
## upcxx modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\t - Adds upcxx to your path"
}

module-whatis   "upcxx-2020.3.0"

set      hpcc_modules    $::env(HPCC_MODULES)

# Setup PATHs
prepend-path    PATH            ${hpcc_modules}/upcxx/2020.3.0/bin
setenv	UPCXX_INSTALL	${hpcc_modules}/upcxx/2020.3.0
