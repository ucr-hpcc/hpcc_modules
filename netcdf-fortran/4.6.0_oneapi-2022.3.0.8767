#%Module1.0#####################################################################
##
## netcdf-fortran modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tnetcdf-fortran- Adds intel compiled netcdf-fortran to your path"
}

module-whatis	"netcdf-fortran-4.6.0_oneapi-2022.3.0.8767"

set      hpcc_modules    $::env(HPCC_MODULES)

prepend-path    PATH              ${hpcc_modules}/netcdf-fortran/4.6.0_oneapi-2022.3.0.8767/bin
prepend-path    LIBRARY_PATH      ${hpcc_modules}/netcdf-fortran/4.6.0_oneapi-2022.3.0.8767/lib
prepend-path    LD_LIBRARY_PATH   ${hpcc_modules}/netcdf-fortran/4.6.0_oneapi-2022.3.0.8767/lib
prepend-path    MANPATH           ${hpcc_modules}/netcdf-fortran/4.6.0_oneapi-2022.3.0.8767/share/man
prepend-path    CPATH             ${hpcc_modules}/netcdf-fortran/4.6.0_oneapi-2022.3.0.8767/include
