#%Module1.0#####################################################################
##
## aspera modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\taspera - Adds ascp to your path"
}

module-whatis	"aspera-4.2.4.265"

set      hpcc_modules    $::env(HPCC_MODULES)

prepend-path    PATH            ${hpcc_modules}/aspera/4.2.4.265/bin
setenv	        ASPERAKEY	${hpcc_modules}/aspera/4.2.4.265/etc/asperaweb_id_dsa.openssh	

