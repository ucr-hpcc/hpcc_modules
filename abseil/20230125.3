#%Module1.0#####################################################################
##
## Abseil modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\t - Adds Abseil to your path"
}

module-whatis   "Abseil-20230125.3"

set      hpcc_modules    $::env(HPCC_MODULES)

# Setup PATHs
prepend-path    CPATH           ${hpcc_modules}/abseil/20230125.3/include
prepend-path    C_INCLUDE_PATH  ${hpcc_modules}/abseil/20230125.3/include
prepend-path    LIBRARY_PATH    ${hpcc_modules}/abseil/20230125.3/lib64
prepend-path    LD_LIBRARY_PATH ${hpcc_modules}/abseil/20230125.3/lib64

