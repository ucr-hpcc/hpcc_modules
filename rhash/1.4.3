#%Module1.0#####################################################################
##
## Rhash modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\t - Adds Rhash to your path"
}

module-whatis   "rhash-1.4.3"

set      hpcc_modules    $::env(HPCC_MODULES)

# Setup PATHs
prepend-path    PATH            ${hpcc_modules}/rhash/1.4.3/bin
prepend-path    MANPATH         ${hpcc_modules}/rhash/1.4.3/share/man
prepend-path    LIBRARY_PATH    ${hpcc_modules}/rhash/1.4.3/lib
prepend-path    LD_LIBRARY_PATH ${hpcc_modules}/rhash/1.4.3/lib

