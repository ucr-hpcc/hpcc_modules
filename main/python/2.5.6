#%Module1.0#####################################################################
##
## Python2.5 modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tpython2 - Adds python2.5 to your path"
}

module-whatis	"python-2.5.6"

# for Tcl script use only
set	version		2.5.6
set	prefix		/usr/local/Modules/2.5.6
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share


prepend-path    PATH   		/opt/Python/2.5.6/bin
prepend-path    MANPATH  	/opt/Python/2.5.6/share/man

module		use		/usr/local/Modules/versions

