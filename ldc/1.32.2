proc ModulesHelp { } {
        global version prefix

        puts stderr "\tldc - Adds D compiler to your path"
}

module-whatis	"ldc-1.32.2"

set      hpcc_modules    $::env(HPCC_MODULES)

prepend-path    PATH            ${hpcc_modules}/ldc/1.32.2/bin
prepend-path    LD_LIBRARY_PATH	${hpcc_modules}/ldc/1.32.2/lib

