#%Module1.0#####################################################################
##
## bioseqzip modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tbioseqzip- Adds bioseqzip to your path"
}

module-whatis	"bioseqzip-2020-05-05"

set      hpcc_modules    $::env(HPCC_MODULES)

prepend-path    PATH            ${hpcc_modules}/bioseqzip/2020-05-05/bin

