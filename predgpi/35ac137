#%Module1.0#####################################################################
##
## predgpi modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tpredgpi - Adds predgpi for predicting GPI anchor site to your path"
}

module-whatis	"predgpi-35ac137"

set      hpcc_modules    $::env(HPCC_MODULES)

module load miniconda3

prepend-path    PATH            ${hpcc_modules}/predgpi/35ac137
setenv	PREDGPI_HOME	${hpcc_modules}/predgpi/35ac137

