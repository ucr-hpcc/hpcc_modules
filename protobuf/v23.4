#%Module1.0#####################################################################
##
## Protobuf modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\t - Adds Protobuf to your path"
}

module-whatis   "Protobuf-v23.4"

set      hpcc_modules    $::env(HPCC_MODULES)

# Setup PATHs
prepend-path    PATH            ${hpcc_modules}/protobuf/v23.4/bin
prepend-path    CPATH           ${hpcc_modules}/protobuf/v23.4/include
prepend-path    LIBRARY_PATH    ${hpcc_modules}/protobuf/v23.4/lib64
prepend-path    LD_LIBRARY_PATH ${hpcc_modules}/protobuf/v23.4/lib64
prepend-path    CMAKE_PREFIX_PATH ${hpcc_modules}/protobuf/v23.4
