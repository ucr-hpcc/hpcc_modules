#%Module1.0#####################################################################
##
## velvet modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tvelvet- Adds velvet to your path"
}

module-whatis	"velvet-1.2.10-bigassembly"

set      hpcc_modules    $::env(HPCC_MODULES)

prepend-path    PATH            ${hpcc_modules}/velvet/1.2.10-bigassembly

