#%Module1.0#####################################################################
##
## genometools modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tgenometools - Adds genometools 1.6.5 to your path"
}

module-whatis   "genometools-1.6.5"

set      hpcc_modules    $::env(HPCC_MODULES)

prepend-path    PATH             ${hpcc_modules}/genometools/1.6.5/bin
prepend-path    LD_LIBRARY_PATH  ${hpcc_modules}/genometools/1.6.5/lib

