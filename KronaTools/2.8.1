#%Module1.0#####################################################################
##
## KronaTools modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tKronaTools - Adds KronaTools to your path"
}

module-whatis	"KronaTools-2.8.1"

set      hpcc_modules    $::env(HPCC_MODULES)
set      modules_dir     $::env(MODULES_DIR)

# for Tcl script use only
set	version		3.2.9
set	prefix		${modules_dir}/${version}
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share


prepend-path    PATH            ${hpcc_modules}/KronaTools/2.8.1/bin


