#%Module1.0#####################################################################
##
## VMD modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\t - Adds VMD to your path"
}

module-whatis   "VMD-1.9.3"

set      hpcc_modules    $::env(HPCC_MODULES)

# Setup PATHs
prepend-path    PATH            ${hpcc_modules}/vmd/1.9.3/bin
prepend-path    LIBRARY_PATH    ${hpcc_modules}/vmd/1.9.3/lib/vmd
prepend-path    LD_LIBRARY_PATH ${hpcc_modules}/vmd/1.9.3/lib/vmd

