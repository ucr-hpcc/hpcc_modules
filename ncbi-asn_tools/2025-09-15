#%Module1.0#####################################################################
##
## ncbi asn conversion tools modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tncbi-asn_tools - Adds ncbi asn processing tools (eg asn2all tbl2asn) to your path"
}

module-whatis	"ncbi-asn_tools"

set      hpcc_modules    $::env(HPCC_MODULES)

prepend-path    PATH            ${hpcc_modules}/ncbi-asn_tools/2025-09-15

