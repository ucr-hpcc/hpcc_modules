#%Module1.0#####################################################################
##
## jupyterlab modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tjupyterlab- Adds jupyterlab to your path"
}

module-whatis	"jupyterlab-4.0.1"

set      hpcc_modules    $::env(HPCC_MODULES)

# Load miniconda
module load miniconda3

if { [module-info mode load] || [module-info mode switch2] } {
    puts stdout "module load miniconda3; conda activate --stack ${hpcc_modules}/jupyterlab/4.0.1;"
} elseif { [module-info mode remove] && ![module-info mode switch3] } {
    puts stdout "conda deactivate;"
}

