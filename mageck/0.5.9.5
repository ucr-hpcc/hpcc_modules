#%Module1.0#####################################################################
##
## mageck modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tmageck - Adds mageck tools to your path"
}

module-whatis	"mageck-0.5.9.5"

set      hpcc_modules    $::env(HPCC_MODULES)

if { [module-info mode load] || [module-info mode switch2] } {
    puts stdout "module load miniconda3; conda config --set env_prompt '(\$(echo {default_env} |  perl -p -e \"s#.+pkgs/##\")) '; conda activate --stack ${hpcc_modules}/mageck/0.5.9.5/env;"
} elseif { [module-info mode remove] && ![module-info mode switch3] } {
    puts stdout "conda deactivate;"
}

