#%Module1.0#####################################################################
##
## less modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tless - Adds less to your path"
}

module-whatis	"less-608"

set      hpcc_modules    $::env(HPCC_MODULES)

prepend-path    PATH            ${hpcc_modules}/less/608/bin

