#%Module1.0#####################################################################
##
## cite-seq-count modulefile
#
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tcite-seq-count - cite-seq-count to your path"
}

module-whatis	"cite-seq-count-1.4.5"

set      hpcc_modules    $::env(HPCC_MODULES)

if { [module-info mode load] || [module-info mode switch2] } {
    puts stdout "module load miniconda3; conda activate ${hpcc_modules}/cite-seq-count/1.4.5/env;"
} elseif { [module-info mode remove] && ![module-info mode switch3] } {
    puts stdout "conda deactivate;"
}
