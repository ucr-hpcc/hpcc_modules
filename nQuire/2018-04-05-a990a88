#%Module1.0#####################################################################
##
## nQuire modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tnQuire- Adds nQuire to your path"
}

module-whatis	"nQuire-2018-04-05-a990a88"

set      hpcc_modules    $::env(HPCC_MODULES)

prepend-path    PATH            ${hpcc_modules}/nQuire/2018-04-05-a990a88/bin

