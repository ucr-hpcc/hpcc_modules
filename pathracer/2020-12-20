#%Module1.0#####################################################################
##
## pathracer modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tpathracer - Adds pathracer and spades tools to your path"
}

module-whatis	"pathracer-2020-12-20"

set      hpcc_modules    $::env(HPCC_MODULES)

prepend-path    PATH            ${hpcc_modules}/pathracer/2020-12-20/bin:${hpcc_modules}/pathracer/2020-12-20

