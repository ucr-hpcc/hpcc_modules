#%Module1.0#####################################################################
##
## GenomeMapper modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\t - Adds GenomeMapper to your path"
}

module-whatis   "GenomeMapper-0.4.4"

set      hpcc_modules    $::env(HPCC_MODULES)

# Setup PATHs
prepend-path    PATH            ${hpcc_modules}/genomemapper/0.4.4/bin
setenv          GENOMEMAPPER    ${hpcc_modules}/genomemapper/0.4.4/bin

