#%Module1.0#####################################################################
##
## fastq_demux modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tfastq_demux - Adds fastq_demux modules to your path"
}

module-whatis	"fastq_demux-0.0.1"

set      hpcc_modules    $::env(HPCC_MODULES)

if { [module-info mode load] || [module-info mode switch2] } {
    puts stdout "module load miniconda3; conda config --set env_prompt '(\$(echo {default_env} |  perl -p -e \"s#.+pkgs/##\")) '; conda activate ${hpcc_modules}/fastq_demux/0.0.1;"
} elseif { [module-info mode remove] && ![module-info mode switch3] } {
    puts stdout "conda config --set env_prompt '({default_env}) '; conda deactivate;"
}

