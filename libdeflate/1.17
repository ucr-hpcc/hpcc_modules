#%Module1.0#####################################################################
##
## libdeflate modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tlibdeflate - Adds libdeflate apps and libraries to your path"
}

module-whatis	"libdeflate-1.17"

set      hpcc_modules    $::env(HPCC_MODULES)

prepend-path    PATH              ${hpcc_modules}/libdeflate/1.17/bin
prepend-path    LD_LIBRARY_PATH	  ${hpcc_modules}/libdeflate/1.17/lib64

