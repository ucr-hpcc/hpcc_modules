#%Module1.0#####################################################################
##
## usearch modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tusearch - Adds usearch to your path"
}

module-whatis	"usearch-12"

set      hpcc_modules    $::env(HPCC_MODULES)

prepend-path    PATH            ${hpcc_modules}/usearch/12/bin
prepend-path    PATH            ${hpcc_modules}/usearch/12/scripts

