#%Module1.0#####################################################################
##
## code-server modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\t - Adds code-server to your path"
}

module-whatis   "code-server-4.19.1"

set      hpcc_modules    $::env(HPCC_MODULES)

# Setup PATHs
prepend-path    PATH            ${hpcc_modules}/code-server/4.19.1/bin
prepend-path    LIBRARY_PATH    ${hpcc_modules}/code-server/4.19.1/lib
prepend-path    LD_LIBRARY_PATH ${hpcc_modules}/code-server/4.19.1/lib

