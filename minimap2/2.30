#%Module1.0#####################################################################
##
## minimap2 modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tminimap2 - Adds minimap2 to your path"
}

module-whatis	"minimap2-2.30"

set      hpcc_modules    $::env(HPCC_MODULES)

prepend-path    PATH            ${hpcc_modules}/minimap2/2.30/bin
prepend-path	MANPATH		${hpcc_modules}/minimap2/2.30/share/man

