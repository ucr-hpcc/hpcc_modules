proc ModulesHelp { } {
        global version prefix

        puts stderr "\thmmer - Adds hmmer3 enabled apps to your path"
}

module-whatis	"hmmer-3.4"

set      hpcc_modules    $::env(HPCC_MODULES)

prepend-path    PATH            ${hpcc_modules}/hmmer/3.4/bin
prepend-path    MANPATH         ${hpcc_modules}/hmmer/3.4/share/man

